{
    "name": "kilip/laravel-alice",
    "description": "Laravel Alice Integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Kilip\\Laravel\\Alice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Kilip\\Laravel\\Alice\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "./vendor/bin/php-cs-fixer fix"
    },
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "psr/log": "^1.0",
        "theofidry/alice-data-fixtures": "^1.0.1|^1.1|^1.2"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.2|^1.3|^1.4",
        "doctrine/orm": "^2.6.6|^2.7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "laravel-doctrine/orm": "^1.4|^1.5.7|^1.6",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.30",
        "phpstan/phpstan-doctrine": "^0.12.16",
        "phpstan/phpstan-phpunit": "^0.12.11",
        "phpunit/phpunit": "^8.5"
    }
}
